<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--
    namespace: 名称空间
-->
<mapper namespace="com.itheima.dao.AddressDao">
    <insert id="save" parameterType="">
        insert into tab_address
        (
        aid,
        uid,
        contact,
        address,
        telephone,
        is_default
        ) values
        (
        #{aid},
        #{uid},
        #{contact},
        #{address},
        #{telephone},
        #{isDefault}
        )
    </insert>
    <select id="findByUid">

    </select>
</mapper>
